
@{
    ViewBag.Title = "SendData3";
}

<h2>SendData3</h2>

<div class="panel-body panel-success">

    <table class="table table-bordered table-striped">
        <tr>
            <th>EmpId</th>
            <th>EmpName</th>
            <th>EmpSalary</th>
        </tr>
        @foreach (var item in ViewBag.listEmp)
        {
            <tr>
                <td>@item.EmpId</td>
                <td>@item.EmpName</td>
                <td>@item.EmpSalary</td>
            </tr>
        }
    </table>
</div>
